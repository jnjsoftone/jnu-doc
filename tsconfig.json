{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": [ "es2020" ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./types",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": [ "src/**/*", "_backups/cheer/cheer_0.ts" ],
  "exclude": [ "node_modules", "**/*.test.ts" ]
}